{
  "name": "quirk",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=v18.16.0"
  },
  "scripts": {
    "check": "pnpm lint:check && pnpm format:check && pnpm typecheck",
    "dev": "wrangler pages dev --compatibility-date=2023-05-10 -- pnpm vite",
    "build": "vite build",
    "preview": "vite preview",
    "fix": "pnpm lint:fix && pnpm format:fix",
    "format:check": "prettier --check \"**/*.{ts,tsx,cjs,mjs,json,md,mdx,css}\" --cache --ignore-path .gitignore",
    "format:fix": "prettier --write \"**/*.{ts,tsx,cjs,mjs,json,md,mdx,html,css}\" --cache --ignore-path .gitignore",
    "lint:check": "eslint . --cache --cache-strategy content --ignore-path .gitignore",
    "lint:fix": "eslint . --fix --ignore-path .gitignore",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@fontsource/inter": "^5.0.2",
    "@lucia-auth/adapter-mysql": "^1.1.1",
    "@lucia-auth/oauth": "^1.1.1",
    "@planetscale/database": "^1.7.0",
    "@tanstack/router": "^0.0.1-beta.86",
    "cookie": "^0.5.0",
    "lucia-auth": "^1.7.1",
    "prisma": "^4.15.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230518.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.0.2",
    "@types/cookie": "^0.5.1",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.8",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "wrangler": "^3.0.1"
  }
}
