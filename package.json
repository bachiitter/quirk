{
  "name": "quirk",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=v18.16.0"
  },
  "scripts": {
    "db:generate": "prisma generate",
    "db:push": "prisma db push --skip-generate",
    "check": "pnpm lint:check && pnpm format:check && pnpm typecheck",
    "dev": "wrangler pages dev --compatibility-date=2023-05-10 -- pnpm vite",
    "build": "vite build",
    "preview": "vite preview",
    "fix": "pnpm lint:fix && pnpm format:fix",
    "format:check": "prettier --check \"**/*.{ts,tsx,cjs,mjs,json,md,mdx,css}\" --cache --ignore-path .gitignore",
    "format:fix": "prettier --write \"**/*.{ts,tsx,cjs,mjs,json,md,mdx,html,css}\" --cache --ignore-path .gitignore",
    "lint:check": "eslint . --cache --cache-strategy content --ignore-path .gitignore",
    "lint:fix": "eslint . --fix --ignore-path .gitignore",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@fontsource/inter": "^5.0.3",
    "@headlessui/react": "^1.7.15",
    "@heroicons/react": "^2.0.18",
    "@hookform/resolvers": "^3.1.0",
    "@lucia-auth/adapter-mysql": "2.0.0-beta.1",
    "@lucia-auth/oauth": "2.0.0-beta.0",
    "@planetscale/database": "^1.7.0",
    "@radix-ui/react-avatar": "^1.0.3",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@tanstack/react-query": "^4.29.12",
    "@tanstack/router": "^0.0.1-beta.86",
    "@trpc/client": "^10.30.0",
    "@trpc/react-query": "^10.30.0",
    "@trpc/server": "^10.30.0",
    "cookie": "^0.5.0",
    "kysely": "^0.25.0",
    "kysely-planetscale": "^1.3.0",
    "lucia": "2.0.0-beta.0",
    "prisma": "^4.15.0",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^7.44.3",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230518.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.0.2",
    "@types/cookie": "^0.5.1",
    "@types/node": "^20.3.0",
    "@types/react": "^18.2.11",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "autoprefixer": "^10.4.14",
    "class-variance-authority": "^0.6.0",
    "clsx": "^1.2.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "lucide-react": "^0.242.0",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "tailwind-merge": "^1.13.1",
    "tailwindcss": "^3.3.2",
    "tailwindcss-animate": "^1.0.6",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "wrangler": "^3.1.0"
  }
